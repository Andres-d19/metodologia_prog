Algoritmo RAICES 
	definir A,B,C COMO REALES;
	Definir x1,x2 como reales;
	Escribir "INGREAR ELVALOR DE A " ;
	Leer A;
	Escribir "INGREAR ELVALOR DE B " ;	
	Leer B ;
	Escribir "INGREAR ELVALOR DE C " ;
	Leer C ;
	x1<-(-b+raiz(b*b-4*a*c))/(2*a);
	x2<-(-b-raiz(b*b-4*a*c))/(2*a);
	escribir "resultado" ,x1;

FinAlgoritmo

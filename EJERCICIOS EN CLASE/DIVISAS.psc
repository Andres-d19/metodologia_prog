Algoritmo divisas
	Definir cantidad,euro como real ;
	Definir mitad,dolar como real;
	Escribir "ingresa la cantida de pesos";
	Leer cantidad;
    mitad<-cantidad/2; 
	dolar<-mitad/17.85;
	Escribir " la cantida en dolares es:$",dolar;
	euro<-mitad/18.93;
	Escribir " EN EUROS SON:£",euro;
	
FinAlgoritmo
